@{
    ViewData[TitleKey] = "Developers";
    ViewData[PaginationCurrentPage] = Model.PaginationModel.CurrentPage;
    ViewData[PaginationMaxPageCount] = Model.PaginationModel.MaxPageCount;
    ViewData[PaginationDisplayCount] = Model.PaginationModel.DisplayCount;
}
@model DevelopersViewModel
<section class="events-list-area section-gap event-page-lists" style="padding: 60px 0;">
    <div class="container">
        <div class="row">
            @if(Model.PaginationModel.Collection != default(ICollection<User>))
              {
                foreach(var user in Model.PaginationModel.Collection)
                {
                          <div class="col-lg-3 col-md-6 single-blog">
                              <div class="thumb">
                                <img class="img-fluid" src="~/@user.ImagePath" alt="user image" />
                              </div>
                              <h5 style="margin-top:8px;">@user.UserName</h5>
                              <p>@user.Specialization<br/>Joined : @user.CreatedAt.ToString(DateFormat)</p>
                              <a
                                asp-action="profile"
                                asp-controller="home"
                                asp-route-id="@user.Id"
                                target="_blank"
                                class="
                                  details-btn
                                  d-flex
                                  justify-content-center
                                  align-items-center
                                "
                                ><span class="details">Details</span
                                ><span class="lnr lnr-arrow-right"></span
                              ></a>
                    </div>
                }
              }
        </div>
    </div>
    @{
        await Html.RenderPartialAsync("_PaginationPartial");
    }
        </section>
