@{
    Layout = "_DashboardLayout";
    ViewData[PaginationCurrentPage] = Model.UsersPagination.CurrentPage;
    ViewData[PaginationMaxPageCount] = Model.UsersPagination.MaxPageCount;
    ViewData[PaginationDisplayCount] = Model.UsersPagination.DisplayCount;
}
@model MembersDashboardVM
<section class="events-list-area section-gap event-page-lists" style="padding: 60px 0;">
    <div class="container">
        <div class="row align-items-center">
    @if (Model.UsersPagination.Collection.Count != 0)
    {
        @foreach(User user in Model.UsersPagination.Collection)
        {
                    <div class="col-lg-6 pb-30">
                        <div class="single-carusel row align-items-center">
                            <div class="col-12 col-md-6 thumb">
                  <a asp-action="profile" asp-controller="home" asp-route-id="@user.Id">
                        <img class="custom-img-size clickable" src="~/@user.ImagePath" alt="Profile Img" />
                  </a>
                            </div>
                            <div class="detials col-12 col-md-6">
                                <h4>@user.UserName</h4>
                                <h4>@user.Specialization</h4>
                                                  <a class="mdi mdi-delete-forever clickable light" asp-action="deletemember" asp-controller="dashboard" asp-route-id="@user.Id" is-delete="1"></a>
                            </div>
                        </div>
                    </div>
        }
    }
        </div>
    </div>
    @{
        await Html.RenderPartialAsync("_PaginationPartial");
    }
        </section>
<!--End Profile Card-->
