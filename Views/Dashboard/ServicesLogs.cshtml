@{
    Layout = "_DashboardLayout";
}
@model ServicesLogsVM
              <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body bg">
                    <h4 class="card-title">Services Logs</h4>
                    <div class="table-responsive">
                      <table class="table bg">
                        <thead>
                          <tr>
                            <th>Service Name</th>
                            <th>Username</th>
                            <th>Action</th>
                            <th>Created At</th>
                          </tr>
                        </thead>
                        <tbody>
                            @foreach(var log in Model.Logs)
                            {
                              
                              <tr class="clickable-bg">
                                    <td>@log.ServiceName</td>
                                    <td>@log.UserName</td>
                                    <td>@log.Action</td>
                                    <td>@log.CreatedAt</td>
                              </tr>
                            }
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>